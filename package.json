{
  "name": "@dom111/flow-free",
  "version": "0.1.0",
  "description": "A TS/SCSS implementation of Flow Free",
  "scripts": {
    "build": "npm run build:esbuild && npm run build:prettier:write",
    "build:esbuild": "node ./esbuild.js",
    "build:esbuild:watch": "node ./esbuild.js watch",
    "build:prettier:check": "prettier -c .",
    "build:prettier:write": "prettier -w .",
    "build:watch": "npm run build:esbuild:watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run build:watch"
  },
  "keywords": [
    "flow",
    "game"
  ],
  "author": "dom111 <dom111@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.15.0",
    "esbuild-sass-plugin": "^2.3.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@dom111/element": "^0.2.0"
  }
}
